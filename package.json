{
  "name": "trainee-ui-app",
  "version": "0.34.1",
  "private": true,
  "dependencies": {
    "@aws-amplify/ui-components": "^1.7.3",
    "@aws-amplify/ui-react": "^1.2.10",
    "@cypress/code-coverage": "^3.9.9",
    "@cypress/instrument-cra": "^1.4.0",
    "@cypress/react": "^5.8.0",
    "@cypress/webpack-dev-server": "^1.4.0",
    "@material-ui/core": "^4.12.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@sentry/browser": "^6.8.0",
    "@types/node": "^14.14.32",
    "@types/qrcode.react": "^1.0.1",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "@types/react-gtm-module": "^2.0.0",
    "@types/react-helmet": "^6.1.2",
    "@types/react-router-dom": "^5.1.5",
    "@types/yup": "0.29.13",
    "aws-amplify": "^4.2.4",
    "axios": "^0.21.1",
    "browser-update": "^3.3.25",
    "cypress": "^7.3.0",
    "cypress-otp": "^1.0.3",
    "dayjs": "^1.10.6",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-prettier": "^3.4.0",
    "formik": "^2.2.6",
    "nhsuk-frontend": "^3.1.0",
    "nhsuk-react-components": "^1.2.8",
    "prettier": "^2.3.0",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.2",
    "react-cookie-consent": "^6.2.4",
    "react-dom": "^17.0.2",
    "react-gtm-module": "^2.0.11",
    "react-helmet": "^6.1.0",
    "react-loading": "^2.0.3",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-test-renderer": "^17.0.2",
    "redux": "^4.0.5",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "sass": "^1.32.13",
    "stylelint": "^13.12.0",
    "typescript": "^4.2.4",
    "yup": "^0.32.9"
  },
  "scripts": {
    "start": "react-scripts -r @cypress/instrument-cra start",
    "prebuild": "npm run generate-build-version",
    "generate-build-version": "node generate-build-version",
    "build": "react-scripts build",
    "test": "react-scripts test --silent --watchAll=false --coverage --testResultsProcessor jest-sonar-reporter",
    "cypress": "cypress run",
    "cypress:open": "cypress open",
    "ct:open": "npx cypress open-ct",
    "ct": "npx cypress run-ct",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .ts,.tsx,.js --fix",
    "stylelint": "stylelint \"src/**/*.scss\" \"src/**/*.css\" --fix",
    "report:merge": "mochawesome-merge cypress/results/json/*.json > cypress/results/index.json",
    "report:generate": "marge cypress/results/index.json --reportDir cypress/reports --assetsDir cypress/reports/assets --reportPageTitle index.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run stylelint"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^26.0.23",
    "@types/react-test-renderer": "^17.0.1",
    "@types/redux-mock-store": "^1.0.2",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.2",
    "babel-plugin-istanbul": "^6.0.0",
    "cypress-multi-reporters": "^1.4.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.30.0",
    "husky": "^6.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "mocha": "^9.0.2",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "stylelint-prettier": "^1.2.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/**/*.{ts,tsx}",
      "!**/src/mock-data/*.{js,jsx,ts,tsx}",
      "!**/src/redux/store/*.{js,jsx,ts,tsx}",
      "!**/src/redux/types.ts",
      "!**/src/components/forms/formr-part-b/Sections/SectionProps.ts",
      "!**/src/*.{js,ts}",
      "!**/src/index.tsx",
      "!**/src/models/*.ts",
      "!**/src/redux/reducers/index.ts",
      "!**/src/**/*.cy-test.{js,jsx,ts,tsx}"
    ],
    "coverageReporters": [
      "json",
      "html",
      "lcov",
      "text"
    ],
    "resetMocks": false
  },
  "jestSonar": {
    "sonar56x": true,
    "reportPath": ".",
    "reportFile": "coverage/sonar-report.xml",
    "indent": 4
  }
}
